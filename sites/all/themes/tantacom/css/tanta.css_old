/*Generic*/
.ftr{float:right}
.ftl{float:left}
.both{clear:both}
.clear:after{content:".";clear:both;display:block;visibility:hidden;height:0}
.clearfix{height:1%}
.dnone{display:none}
a{text-decoration:none;}
a img{border:0;}

span.boton { display: block; margin-bottom: 1em }

/**/
aside, header, nav, footer, section, hgroup, article{display:block;}

/*Layout*/
html {}
body { font-size:62.5%; font-family:Arial, Helvetica, sans-serif;}
#wrapper { width:98%;}
	aside { position:fixed; top:0; left:0; width:26%; background:#1f91d0 url(../images/f_aside.png) repeat-y 100% 0; height:100%;}
		aside header { height:7em; position:relative; z-index:2;}
		aside nav { position:relative; z-index:2;}
		aside footer { z-index:1; position:absolute; bottom:0; right:4em;}
	section { width:65%; margin-left:29%; background-color:#fff;}
	
	.colAB { width:100%; margin:0;}
		.colA { width:50%; float:left;}
		.colB { width:50%; float:right;}
	.colAB:after { content:" "; display:block; height:0; clear:both; visibility:hidden;}
	
	.linked { position:relative; width:100%; margin:0 0 1em 0; border-bottom:1px solid #d3e9f6; font:2.3em 'Humanst521LtBTLight', Arial, Helvetica, sans-serif; color:#1f91d0;}
		.linked p { margin:0 0 .7em 0; padding:0 9.3em 0 0;}
		.linked a { position:absolute; right:0; top:15%; font-size:.55em !important;}
	
/*Design*/
a{color:#1f91d0;}
aside{}
	nav { padding-bottom:1em; background:url(../images/f_aside.png) repeat-y 100% 0; text-align:right;}
		nav header { margin:0 4em 0 2em; padding-top:3em;}
			nav header a:link{padding:0;}
			#logo { display:block; float:right;}
		nav ul { padding:0; padding-top:4em;}
			nav li { list-style:none; margin:.8em 0;}
				nav a:link,nav a:visited { display:inline-block; padding:0 2.3809em 0 .952em; height:1.238em; font-family:'Humanst521LtBTLight', Arial, Helvetica, sans-serif; font-size:2.1em; color:#fff; text-shadow: 1px 1px 0px #105e88; line-height:.952em;}
				nav a:hover { text-decoration:none; color:#063a56;}
				nav a.sel { color:#1f91d0; background:#cdeffd url(../images/f_nav_sel.png) no-repeat 0 0; text-shadow: 1px 1px 0px #7dc2e7}
	footer { text-align:right; }
		footer ul { display:inline-block; padding:0; }
			footer li { list-style:none; padding:2em 1em 0 0; margin-bottom:2em; background:url(../images/f_footer_aside.png) no-repeat right 0;}
				footer li li { background:none; padding:0; margin:0;}
					footer .map, footer .tlf, footer .mail { clear:right; float:right; padding-left:2em; margin-bottom:1em;}
						footer .map { background:url(../images/f_footer_map.png) no-repeat 0 0;}
						footer .tlf { background:url(../images/f_footer_tlf.png) no-repeat 0 0;}
						footer .mail { background:url(../images/f_footer_mail.png) no-repeat 0 0;margin:0;}
				
				footer a:link,footer a:visited { font-size:1.2em; font-weight:bold; color:#063a56; text-shadow: 1px 1px 0px #60b1de}
				footer a:hover { text-decoration:underline;}
				footer a.sel { color:#fff; text-shadow: 1px 1px 0px #063a56}
				
				footer span { font-size:1.2em;color:#ace0f6;text-shadow: 1px 1px 0px #156e9f}
section{}
	section header { margin:0 0 2.1em 0; border-bottom:1px solid #d3e9f6;}
		.lang_social { position:relative; float:left; width:100%; border-bottom:1px solid #d3e9f6;}
			.lang { padding:.363em .727em; background-color:#97d7f7; color:#fff; text-transform:uppercase; font-style:1.1em;}
			.social { float:right; padding:0;}
				.social li { float:left;margin-left:1em;list-style-type:none;}
					.social span { display:block; margin:1em 0 0 0; font-size:1.2em; color:#717171;}
					.social a { display:block; height:3.3em; width:3em; background-repeat:no-repeat; background-position:50% 10px; -webkit-transition:background-position 0.7s ease; -moz-transition:background-position 0.7s ease; transition:background-position 0.7s ease; text-indent:999px; overflow:hidden; }
						.social a:hover { background-position:50% 5px;}
					
					.twitter a { background-image:url(../images/i_twitter.png);}
					.linkedin a { background-image:url(../images/i_linkedin.png);}
					.rss a { background-image:url(../images/i_rss.png);}
					
		section header hgroup { clear:left; padding:1em 0 1.5em;}
			section header h1 { margin-bottom:.357em; padding:0; font-size:2.8em; color:#1f91d0; font-family:'Humanst521LtBTLight', Arial, Helvetica, sans-serif; text-shadow: 1px 1px 0px #b4c4cc;}
			section header h2 { font-size:1.9em; color:#515151; font-style:italic;margin:0;}
			section header h3 { margin-bottom:.357em; margin-top:.3em; padding:0; font-size:2.2em; color:#1f91d0; font-family:'Humanst521LtBTLight', Arial, Helvetica, sans-serif;}
			section header h4 { font-size:1.6em; color:#515151; font-style:italic;margin:0;}

.wysiwyg { font-size:1.3em; color:#515151;}
	.destacado, .detalleC div { background:#cdeffd; padding:1.5em 1.3em; color:#1f91d0 !important; font-weight:bold;}
		.destacado p, .detalleC div p { background:#cdeffd; padding:1.5em 1.3em; color:#1f91d0 !important; font-weight:bold;}
		
	.detalleC div { margin-bottom: 1em ; padding:2em 2em 1.5em;}	
	
	.detalleC div p { padding:0 }
	

/**** botones border-diagonal ****/

.btn{margin:0;padding:0;border:0;font-family:Arial, Helvetica, sans-serif;display:inline-block;background:url(../images/f_btn.png) no-repeat 0 0;padding-left:4em;text-decoration:none; font-weight:bold;color:#9a7a20 !important;line-height:1.25em;}
.btn input {font-size:1em; cursor:pointer}
a.btn{font-size:1.2em;}
.btn_blue{background-image:url(../images/f_btn_blue.png);color:#5690cd !important;}
a.btn:hover{text-decoration:underline;}

/**** breadcrumb ****/

	.breadcrumb { background:none; margin:0 0 1.2em 0; text-align:left;}
		.breadcrumb ul { padding:0 0 0 1em; background:url(../images/f_breadcrumb.gif) no-repeat 0 .35em !important; font:1.2em Arial, Helvetica, sans-serif; color:#929292;}
		.breadcrumb li { margin:0 .3em 0 0; float:left;}
		.breadcrumb li a { font:1em Arial, Helvetica, sans-serif; color:#1f91d0; text-shadow:none; height:auto; padding:0; line-height:normal;}
		
	.breadcrumb ul:after { content:" "; display:block; clear:both; height:0; visibility:hidden;}
		
/**** paginador ****/


.pagination{clear:both;width:100%;background-color:#cdeffd;margin:2em 0;}
	.pagination .prev, .pagination .next{display:block;font-family:arial;border-radius:2px 0px 2px 0px;color:#063a56;font-size:1.2em;font-weight:bold;padding:.25em 2.083em .333em;text-shadow: 1px 1px 0px #cdeffd;text-decoration:none;}
	.pagination .prev{float:left;background:url(../images/f_prev_izq.png) no-repeat 0 100%,url(../images/f_prev_der.png) no-repeat 100% 0;padding-left:3.333em;background-color:#7fd3f5;}
	.pagination .next{float:right;background:url(../images/f_next_izq.png) no-repeat 0 100%,url(../images/f_next_der.png) no-repeat 100% 0;padding-right:3.333em;background-color:#7fd3f5;}
	.pagination ul{padding:0;text-align:center;}
		.pagination li{list-style-type:none;display:inline;}
			.pagination li a, .pagination .sel span{font-size:1.2em;font-weight:bold;color:#063a56;padding:0 .416em;line-height:1.8em;}
			.pagination .sel a, .pagination .sel span{color:#7cd1f3;}


/******pager de views******/

	.item-list{clear:both;width:100%;background-color:#cdeffd;margin:2em 0;}
		.item-list .pager .pager-first, .item-list .pager .pager-previous, .item-list .pager .pager-last, .item-list .pager .pager-next{display:block;font-family:arial;border-radius:2px 0px 2px 0px;color:#063a56;font-size:1.2em;font-weight:bold;padding:.25em 2.083em .333em;text-shadow: 1px 1px 0px #cdeffd;text-decoration:none;}

		.item-list .pager .pager-first, .item-list .pager .pager-previous{float:left;background:url(../images/f_prev_izq.png) no-repeat 0 100%,url(../images/f_prev_der.png) no-repeat 100% 0;padding-left:3.333em;background-color:#7fd3f5;}

		.item-list .pager .pager-last, .item-list .pager .pager-next{float:right;background:url(../images/f_next_izq.png) no-repeat 0 100%,url(../images/f_next_der.png) no-repeat 100% 0;padding-right:3.333em;background-color:#7fd3f5;}
		.item-list .pager{padding:0;background-color:#FFFFFF;margin:0;}
			.item-list li{list-style-type:none;display:inline;}

				.item-list .pager li .pager-item a, .item-list .pager li .pager-current .first, .item-list .pager li .pager-current .last{font-size:1.2em;font-weight:bold;color:#063a56;padding:0 .416em;line-height:1.8em;}
				.item-list .pager .pager-current .first{color:#7cd1f3;}



/**** aviso legal ****/

	dl.avisoLegal { padding:0 0 1.1em 0; font-size:1.2em; line-height:1.6em;}
		dl.avisoLegal dt { display:inline; float:left; margin:0 .3em 0 0;}
		dl.avisoLegal dd { font-weight:bold;}
	dl.avisoLegal:after { content:" "; display:block; clear:both; height:0; visibility:hidden;}
	
	.wysiwyg h3{font-size:1em;}/*subtitulos aviso legal*/

