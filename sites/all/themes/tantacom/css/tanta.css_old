/**** clientes ****/

	.clientes { margin:0; width:100%;}

	.clientes h3 { margin-bottom:.5em; font:3.1em 'Humanst521LtBTLight', Arial, Helvetica, sans-serif; color:#a67b08; text-shadow: 1px 1px 0px #e0ce70;}
	
	.clientes .colAB { background:url(../images/f_colAB_clientes.gif) repeat-y center 0;}
		.clientes .colA { width:49%; min-height:17.4em; padding:2em 0 0 2em; background:#CDEFFD;}
			.clientes .colA p { margin-right:2em; font-size:1.3em; color:#515151;}
			.clientes .colA .link a { color:#1f91d0; font-weight:bold;}
			.clientes .colA img.floatl { margin-bottom:2em;}
		
		.clientes .colB { width:42%; min-height:17.4em; padding:2em 0 0 2em; background:#1F91D0;}
			.clientes .colB h4 { margin-bottom:.4em; font:2.5em 'Humanst521LtBTLight', Arial, Helvetica, sans-serif; text-shadow: 1px 1px 0px #00528d; color:#cdeffd; line-height:normal;}
			.clientes .colB p { font-size:1.3em; color:#fff; font-style:italic;}
			.clientes .colB img.floatr { margin-bottom:2em; margin-right:2em;}
		
	.clientes .colAB:after { content:" "; display:block; clear:both; height:0; visibility:hidden;}
	
	.clientes article { clear:both; padding:2em 0 2em 3em; background:#f2fafd; border-top:1px solid #fff;}
		.clientes article p { font-size:1.4em; color:#515151;}	
			.clientes article p a { color:#1F91D0; }
				.clientes article p a:hover { text-decoration: underline  }	
		.clientes article h4 { font:2.5em 'Humanst521LtBTLight', Arial, Helvetica, sans-serif; text-shadow: 1px 1px 0px #fff; color:#1f91d0;}
	
	.clientes .archivoGrafico { background:#7ad0f1;}
		.archivoGrafico { display:inline; float:right; width:47.1%; margin:-2em 0 3em 3em; padding:2em 0 0 2em; border:1px solid #fff; border-top:none; border-right:none;}
			.archivoGrafico .slider img { width:96.5%;}
			.archivoGrafico ul { position:relative; list-style:none; margin:0 0 1.5em -.7em; padding:0;}
				.archivoGrafico ul li .preview { position:absolute; left:40%; top:12em; display:block; width:7em; height:9em; background:url(../images/f_preview.png) no-repeat center; text-indent:-9999em;display:none;}
				.archivoGrafico ul li a:hover .preview {display:block;}
			
			.archivoGrafico .tools { position:relative; z-index:3;}
				.archivoGrafico .tools li { position:absolute;}
				.archivoGrafico .tools li.ant { left:0; top:12em;}
				.archivoGrafico .tools li.sig { right:1.1em; top:12em;}
				
		.clientes .archivoGrafico h4 { text-shadow: 1px 1px 0px #1f91d0; color:#fff;}
		
		.clientes article .detalleC ul { list-style: none  }
			.clientes article .detalleC li { padding: 0 .5em 0 .7em; background: url(../images/bullet01.gif) no-repeat 0 .6em; }
		
	.listClientes h3 { font-size:1.9em; color:#515151; font-style:italic; font-weight:normal; margin:0;}
	.listClientes article { margin:2% 0 0 0; padding:2.3%; background:#cdeffd;}
	.listClientes article:after { clear: both; display: block; content: "."; visibility: hidden; height: 0 }
		.listClientes article img.floatl { display:inline; margin:0 3em 3em 0;width:25%;}
		.listClientes article h4 { font:2.5em 'Humanst521LtBTLight', Arial, Helvetica, sans-serif; color:#a67b08; text-shadow: 1px 1px 0px #fff;}
			.listClientes article h4 a { color:#a67b08; }
				.listClientes article h4 a:hover { text-decoration: underline  }
		.listClientes article p { font-size:1.4em; color:#525252;}
		.listClientes article .link { padding:0 2% 0 0; font-size:1em; text-align:right;}
	.listClientes article:after { content:" "; display:block; clear:both; height:0; visibility:hidden;}
	
	section.slider { width:100%; margin:0;}
		section.slider img { padding:0 1.1em; background:url(../images/f_slider-left.png) no-repeat 0 0,url(../images/f_slider-right.png) no-repeat 100% 0;}
	
	
/**** blog ****/

	.blog{}
		.post{margin-top:2em;padding-bottom:2em;border-bottom:1px solid #ede5ce;}
			.post header{margin:0;padding:0;border:0;}
				.post h2{font:2.5em 'Humanst521LtBTLight', Arial, Helvetica, sans-serif;color:#a67b08;text-shadow:1px 1px 0px #e0ce70;margin-bottom:.8em;}
					.post h2 a { color:#a67b08  }
						.post h2 a:hover { text-decoration: underline   }
				.post header p{font-size:1.4em;font-weight:bold;margin-bottom:1.785em;}
			.post .extract{font-size:1.4em;margin-bottom:1.785em;}
		
		.post.newsletter { padding:4% !important;}
			.post.newsletter p { color:#1f91d0; font-size:1.7em;}
			
			.post.newsletter form { font:1.2em Arial, Helvetica, sans-serif; color:#515151;}
				.post.newsletter form label { float:left; width:49%; padding:0 0 5% 0;}
				.post.newsletter form label span { display:block; padding:0 0 3% 0;}
				.post.newsletter form label input { margin:0 0 3% 0;}
				
			.post.newsletter .btn { background-image:url("../images/f_btn_blue.png");}	
			.post.newsletter .btn input { background:none; border:none; color:#2091D0; font-size:.95em; font-weight:bold;}
		
		.post.newsletter02 { padding:4% !important;}
			.post.newsletter02 p { color:#1f91d0; font-size:1.7em;}
			
			.post.newsletter02 form { font:1.2em Arial, Helvetica, sans-serif; color:#515151;}
				.post.newsletter02 form label { display:inline; float:left; width:50%; padding:0 0 5% 0; margin:0 10% 0 0;}
				.post.newsletter02 form label span { display:block; padding:0 0 3% 0;}
				.post.newsletter02 form label input { width:100%; margin:0 0 3% 0;}
				
			.post.newsletter02 .btn { margin-top:5%; background-image:url("../images/f_btn_blue.png");}	
			.post.newsletter02 .btn input { background:none; border:none; color:#2091D0; font-size:.95em; font-weight:bold; cursor:pointer;}
		
		.post.newsletter02:after { content:" "; display:block; clear:both; height:0; visibility:hidden;}
			

/***** quienes somos *********/

